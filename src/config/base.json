{
    "compileOnSave": true,
    "compilerOptions": {
        "lib": [
            "es5",
            "es6"
         ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "skipLibCheck": true,
        "types": ["node"],
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "module": "CommonJS"
    }
}
